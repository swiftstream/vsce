pluginManagement {
    repositories {
        gradlePluginPortal()
        google()
        mavenCentral()
    }
    plugins {
        id("com.android.library") version "{{androidLibraryVersion}}"
        id("org.jetbrains.kotlin.android") version "{{kotlinLibraryVersion}}"
    }
}

dependencyResolutionManagement {
    repositoriesMode.set(RepositoriesMode.FAIL_ON_PROJECT_REPOS)
    repositories {
        gradlePluginPortal()
        google()
        maven { url = uri("https://jitpack.io") }
        mavenCentral()
    }
}

rootProject.name = "{{name}}"

// managed by swiftstreamide: includes-begin
{{#each targets as |target|}}
include(":{{{target}}}")
{{/each}}
// managed by swiftstreamide: includes-end